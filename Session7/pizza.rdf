        <rdfs:domain rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#PizzaTopping"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#Pizza"/>
    </owl:ObjectProperty>
    


