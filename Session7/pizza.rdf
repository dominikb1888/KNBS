    <!-- http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#isIngredientOf">
