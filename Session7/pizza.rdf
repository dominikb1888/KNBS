        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#JalapenoPepper"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#Mozzarella"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#Pepperoni"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#Tomato"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#JalapenoPepper"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#Mozzarella"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#Pepperoni"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#Tomato"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozarella and Tomato Toppings</rdfs:comment>
    </owl:Class>
    


