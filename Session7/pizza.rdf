        <owl:inverseOf rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#isToppingOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#isBaseOf">
