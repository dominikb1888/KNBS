    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#isToppingOf">
