    <!-- http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#isBaseOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#PizzaBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#hasSpiciness">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/dominikb1888/ontologies/2024/4/Pizza#hasTopping">
